{
	"name": "serve",
	"dependencies": {
		"@sap/cds": "^3.31.1",
		"@sap/cds-odata-v2-adapter-proxy": "^1.4.20",
		"@sap/hana-client": "^2.4.182",
		"@sap/xsenv": "^2.2.0",
		"@sap/xssec": "^2.2.5",
		"express": "^4.17.1",
		"compression": "^1.7.4",
		"helmet": "^3.21.3",
		"passport": "^0.4.1",
		"uuid": "^7.0.1"
	},
	"devDependencies": {},
	"scripts": {
		"postinstall": "npm dedupe && node .build.js",
		"startv4": "node ./node_modules/@sap/cds/bin/cds.js serve gen/csn.json",
		"watch": "nodemon -w . -i node_modules/**,.git/** -e cds -x npm run build",
		"start": "node v2server"
	},
	"cds": {
		"requires": {
			"db": {
				"kind": "hana",
				"model": "gen/csn.json"
			}
		}
	}
}
