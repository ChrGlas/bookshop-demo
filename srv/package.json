{
	"name": "serve",
	"dependencies": {
		"@sap/audit-logging": "^3.2.0",
		"@sap/cds": "^4.2.3",
		"@sap/cds-odata-v2-adapter-proxy": "^1.4.47",
		"@sap/hana-client": "^2.6.54",
		"@sap/xsenv": "^3.0.0",
		"@sap/xssec": "^3.0.10",
		"compression": "^1.7.4",
		"express": "^4.17.1",
		"helmet": "^4.1.1",
		"passport": "^0.4.1"
	},
	"engines": {
		"node": "^10"
	},
	"devDependencies": {},
	"scripts": {
		"postinstall": "npm dedupe && node .build.js",
		"startv4": "node ./node_modules/@sap/cds/bin/cds.js serve gen/csn.json",
		"watch": "nodemon -w . -i node_modules/**,.git/** -e cds -x npm run build",
		"build:xs-security": "cds compile srv/ --to xsuaa > xs-security.json",
		"start": "cds run"
	},
	"cds": {
		"requires": {
			"db": {
				"kind": "hana",
				"model": "gen/csn.json"
			},
			"uaa": {
				"kind": "xsuaa"
			}
		},
		"auth": {
			"passport": {
				"strategy": "JWT"
			}
		}
	}
}
