{
    "services": {
        "LaunchPage": {            
            "adapter": {
                "config": {
                    "catalogs": [
                        {
                            "id": "books_catalog",
                            "title": "Books Application Catalog",
                            "tiles": [
                                {
                                    "id": "BooksBrowse",
                                    "title": "Browse Books",
                                    "size": "1×1",
                                    "tileType": "sap.ushell.ui.tile.DynamicTile",
                                    "properties": {
                                        "chipId": "catalogTile_001",
                                        "title": "Browse Books",
                                        "info": "The latest books",
                                        "targetURL": "#Books-manage"
                                    }
                                },
                                {
                                    "id": "AuthorsBrowse",
                                    "title": "Browse Authors",
                                    "size": "1×1",
                                    "tileType": "sap.ushell.ui.tile.StaticTile",
                                    "properties": {
                                        "chipId": "catalogTile_002",
                                        "title": "Browse Authors",
                                        "info": "The list of authors",
                                        "targetURL": "#Authors-manage"
                                    }
                                }
                            ]
                        }
                    ],
                    "groups": [
                        {
                            "id": "books_group",
                            "title": "Books",
                            "isPreset": true,
                            "isVisible": true,
                            "isGroupLocked": false,
                            "tiles": [
                                {
                                    "id": "BrowseBooksApp",
                                    "title": "Browse Books",
                                    "size": "1×1",
                                    "tileType": "sap.ushell.ui.tile.DynamicTile",
                                    "serviceRefreshInterval": 5,
                                    "properties": {
                                        "chipId": "catalogTile_001",
                                        "title": "Browse Books",
                                        "info": "The latest books",
                                        "targetURL": "#Books-manage",
                                        "serviceUrl_cound": "/catalog/Books/$count",
                                        "serviceUrl": "/appconfig/BooksCount.json",
                                        "numberValue": 8,
                                        "numberFactor": "k",
                                        "stateArrow": "Up",
                                        "numberState": "Positive",
                                        "icon": "sap-icon://travel-expense"
                                    }
                                },
                                {
                                    "id": "BrowseAuthorsApp",
                                    "title": "Browse Authors",
                                    "size": "1×1",
                                    "tileType": "sap.ushell.ui.tile.StaticTile",
                                    "serviceRefreshInterval": 5,
                                    "properties": {
                                        "chipId": "catalogTile_002",
                                        "title": "Browse Authors",
                                        "info": "The list of authors",
                                        "targetURL": "#Authors-manage"
                                    }
                                }
                            ]
                        }
                    ]
                }
            }
        },
        "NavTargetResolution": {
            "config": {
                "enableClientSideTargetResolution": true
            }
        },
        "ClientSideTargetResolution": {
            "adapter": {
                "config": {
                    "inbounds": {
                        "actionBooks": {
                            "semanticObject": "Books",
                            "action": "manage",
                            "title": "Browse Books",
                            "signature": {
                                "parameters": {},
                                "additionalParameters": "allowed"
                            },
                            "resolutionResult": {
                                "applicationType": "SAPUI5",
                                "additionalInformation": "SAPUI5.Component=bookshop",
                                "url": "/browse/webapp"
                            },
                            "indicatorDataSource": {
                                "path": "/catalog/Books/$count",
                                "refresh": 600
                            }
                        },
                        "actionBAuthors": {
                            "semanticObject": "Authors",
                            "action": "manage",
                            "title": "Authors",
                            "signature": {
                                "parameters": {},
                                "additionalParameters": "allowed"
                            },
                            "resolutionResult": {
                                "applicationType": "SAPUI5",
                                "additionalInformation": "SAPUI5.Component=authors",
                                "url": "/authors/webapp"
                            }
                        }
                    }
                }
            }
        }
    }
}