{
    "_version": "1.8.0",
    "sap.app": {
        "id": "orders",
        "type": "application",
        "title": "Manage Orders",
        "description": "Sample Application",
        "i18n": "i18n/i18n.properties",
        "dataSources": {
            "mainService": {
                "uri": "/admin/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            }
        }
    },
    "sap.ui5": {
        "dependencies": {
            "libs": {
                "sap.fe": {}
            }
        },
        "models": {
            "": {
                "dataSource": "mainService",
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true,
                    "groupProperties": {
                        "default": {
                            "submit": "Auto"
                        }
                    }
                }
            }
        },
        "routing": {
            "routes": [
                {
                    "name": "OrdersListRoute",
                    "target": "OrdersListTarget",
                    "pattern": ":?query:"
                },
                {
                    "name": "OrdersDetailsRoute",
                    "target": "OrdersDetailsTarget",
                    "pattern": "Orders({key}):?query:"
                },
                {
                    "name": "ItemsRoute",
                    "target": "ItemsTarget",
                    "pattern": "Orders({key}/Items({key2}):?query:"
                },
                {
                    "name": "ShippingAddressRoute",
                    "target": "ShippingAddressTarget",
                    "pattern": "Orders({key}/ShippingAddress({key2}):?query:"
                },
                {
                    "name": "currencyRoute",
                    "target": "currencyTarget",
                    "pattern": "Orders({key}/currency({key2}):?query:"
                }
            ],
            "targets": {
                "OrdersListTarget": {
                    "type": "Component",
                    "id": "OrdersListTarget",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings": {
                            "entitySet": "Orders",
                            "navigation": {
                                "Orders": {
                                    "detail": {
                                        "route": "OrdersDetailsRoute"
                                    }
                                }
                            },
                            "quickVariantSelection": {
                                    "variants": {
                                            "0": {
                                                    "key": "New",
                                                    "annotationPath": "com.sap.vocabularies.UI.v1.SelectionVariant#New"
                                            },
                                            "1": {
                                                    "key": "InProcess",
                                                    "annotationPath": "com.sap.vocabularies.UI.v1.SelectionVariant#InProcess"
                                            },
                                            "2": {
                                                    "key": "Completed",
                                                    "annotationPath": "com.sap.vocabularies.UI.v1.SelectionVariant#Completed"
                                            }
                                    }
                            }
                        }
                    }
                },
                "OrdersDetailsTarget": {
                    "type": "Component",
                    "id": "OrdersDetailsTarget",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "entitySet": "Orders",
                            "navigation": {
                                "Items": {
                                    "detail": {
                                        "route": "ItemsRoute"
                                    }
                                },
                                "ShippingAddress": {
                                    "detail": {
                                        "route": "ShippingAddressRoute"
                                    }
                                },
                                "currency": {
                                    "detail": {
                                        "route": "currencyRoute"
                                    }
                                }
                            }
                        }
                    }
                },
                "ItemsTarget": {
                    "type": "Component",
                    "id": "ItemsTarget",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "entitySet": "OrderItems"
                        }
                    }
                },
                "ShippingAddressTarget": {
                    "type": "Component",
                    "id": "ShippingAddressTarget",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "entitySet": "OrderShippingAddress"
                        }
                    }
                },
                "currencyTarget": {
                    "type": "Component",
                    "id": "currencyTarget",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "entitySet": "Currencies"
                        }
                    }
                }
            }
        }
    },
    "contentDensities": {
        "compact": true,
        "cozy": true
    },
    "sap.ui": {
        "technology": "UI5",
        "fullWidth": true
    },
    "sap.fiori": {
        "registrationIds": [],
        "archeType": "transactional"
    }
}